<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/background"
    tools:context=".fragments.FragmentB">
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/galenus"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.025" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="@string/galenus2ndpage"
        android:textAlignment="center"
        android:textColor="#060143"
        android:textSize="21sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.639" />

    <ImageButton
        android:id="@+id/movetofragmentc"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginEnd="52dp"
        android:layout_marginRight="52dp"
        android:background="@drawable/rectanglebutton"
        android:text="Move to Fragment C"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.927"
        app:layout_constraintStart_toEndOf="@+id/circleshape2"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.503" />

    <ImageButton
        android:id="@+id/movetofragmenta"
        android:layout_width="69dp"
        android:layout_height="62dp"
        android:background="@drawable/backbutton"
        app:layout_constraintBottom_toBottomOf="@+id/movetofragmentc"
        app:layout_constraintEnd_toStartOf="@+id/movetofragmentc"
        app:layout_constraintHorizontal_bias="0.162"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/movetofragmentc"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/rectangleshape"
        android:layout_width="44dp"
        android:layout_height="13dp"

        android:background="@drawable/roundedaccesory"
        app:layout_constraintBottom_toBottomOf="@+id/circleshape2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/circleshape2"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/circleshape"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:background="@drawable/roundedaccesory"
        app:layout_constraintBottom_toBottomOf="@+id/rectangleshape"
        app:layout_constraintEnd_toStartOf="@+id/rectangleshape"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/movetofragmenta"
        app:layout_constraintTop_toTopOf="@+id/rectangleshape"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/circleshape2"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:background="@drawable/roundedaccesory"
        app:layout_constraintBottom_toBottomOf="@+id/movetofragmentc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.095"
        app:layout_constraintStart_toEndOf="@+id/rectangleshape"
        app:layout_constraintTop_toTopOf="@+id/movetofragmentc" />


</androidx.constraintlayout.widget.ConstraintLayout>