<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.FragmentC"
    android:background="@drawable/background">
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/galenus"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.025" />
    <TextView
        android:id="@+id/textView2"
        android:layout_width="237dp"
        android:layout_height="128dp"
        android:gravity="center_horizontal"
        android:text="@string/galenus3"
        android:textAlignment="center"
        android:textSize="20sp"
        android:fontFamily="@font/poppins"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.659"/>

    <ImageButton
        android:id="@+id/movetoHomeSign"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginBottom="68dp"
        android:background="@drawable/rectanglebutton"
        android:text="Move To Fragment B"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.644"
        app:layout_constraintLeft_toLeftOf="@id/circleshape2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.735" />

    <ImageButton
        android:id="@+id/movetofragmentb"
        android:layout_width="74dp"
        android:layout_height="73dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:background="@drawable/backbutton"
        android:text="Move To Fragment B"
        app:layout_constraintBottom_toBottomOf="@+id/movetoHomeSign"


        app:layout_constraintEnd_toStartOf="@+id/rectangleshape"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/movetoHomeSign"
        app:layout_constraintVertical_bias="0.416" />

    <TextView
        android:id="@+id/rectangleshape"
        android:layout_width="44dp"
        android:layout_height="13dp"
        android:background="@drawable/roundedaccesory"
        app:layout_constraintBottom_toBottomOf="@+id/circleshape"

        app:layout_constraintEnd_toEndOf="@id/circleshape"
        app:layout_constraintHorizontal_bias="0.77"
        app:layout_constraintStart_toStartOf="@id/movetofragmentb"
        app:layout_constraintTop_toTopOf="@+id/circleshape"
        app:layout_constraintVertical_bias="0.4854" />

    <TextView
        android:id="@+id/circleshape"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:background="@drawable/roundedaccesory"
        app:layout_constraintBottom_toBottomOf="@+id/circleshape2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.534"
        app:layout_constraintLeft_toLeftOf="@+id/rectangleshape"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/circleshape2"
        app:layout_constraintVertical_bias="0.478" />

    <TextView
        android:id="@+id/circleshape2"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:background="@drawable/roundedaccesory"
        app:layout_constraintBottom_toBottomOf="@+id/movetoHomeSign"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.095"
        app:layout_constraintLeft_toLeftOf="@+id/circleshape"
        app:layout_constraintStart_toEndOf="@+id/circleshape"
        app:layout_constraintTop_toTopOf="@+id/movetoHomeSign"
        app:layout_constraintVertical_bias="0.478" />
</androidx.constraintlayout.widget.ConstraintLayout>